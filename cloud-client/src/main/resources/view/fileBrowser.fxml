<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="my.cloud.client.controller.FileBrowser">
   <children>
      <HBox>
         <children>
            <TextField HBox.hgrow="ALWAYS" fx:id="commandTextField" onAction="#sendCommand"/>
            <Button mnemonicParsing="false" text="Button" onAction="#sendCommand"/>
         </children>
      </HBox>
      <TextArea VBox.vgrow="ALWAYS" fx:id="commandResultTextArea"/>
   </children>
</VBox>
